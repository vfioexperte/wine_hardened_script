def read_args():
    b1 = 0;
    i = 1;
    s1 = "";
    while True:
        if(i >= len(sys.argv)):
            break;
        s2 = sys.argv[i];
        if(s2.find("\\n") != -1):
            a1 = s2.split("\\n");
            for tmp in a1:
                if(i != 1 and tmp.find(" ") != -1):
                    s1 = s1 + " '" + tmp + "' \n";
                else:
                    s1 = s1 + " " + tmp + " \n";

        elif(i != 1 and s2.find(" ") != -1):
            s1 = s1 + " '" + s2 + "' ";
        else:
            s1 = s1 + " " + s2 + " ";

        i = i +1;
    print("read_args: ", s1);
    return s1;

